      "atom-material-ui"
      "atom-material-syntax-light"
    ]
  "easy-motion-redux": {}
  editor:
    fontFamily: "Source Code Pro"
    fontSize: 15
    invisibles: {}
    nonWordCharacters: "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-…_"
