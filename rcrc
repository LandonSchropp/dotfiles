# Configures the dotfiles used by RCM. To see a list of all of the files and their copied
# destinations, run `lsrc`.

EXCLUDES=(
  Brewfile
  bin
  landons_theme.yml
  macos.sh
  rcrc
  readme.md
  themes
  xterm
  *.terminfo
)

export EXCLUDES="${EXCLUDES[@]}"
export DOTFILES_DIRS="$HOME/.dotfiles"
